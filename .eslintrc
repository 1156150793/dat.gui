{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "no-alert": 1,
    "no-console": 1,
    "no-use-before-define": 0,
    "prefer-rest-params": 0,
    "prefer-template": 0,
    "no-mixed-operators": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": 0,
    "global-require": 0,
    "object-shorthand": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-restricted-syntax": 1
  }
}