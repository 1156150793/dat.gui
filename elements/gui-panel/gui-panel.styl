@import '../shared';

#closeButton { 
    
    font();
    font-size: 14px;
    cursor: pointer;
    text-align: center;
    padding: padding*0.4 padding;

}

#container { 

    background: #1a1a1a;
    position: fixed;
    top: 0;
    width: 270px;

    &.docked-false {
        right: 20px;
    }

    &.docked-true {
        right: 0;
        bottom: 0;
    }

}
